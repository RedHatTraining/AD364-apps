package com.demos.event_planner;

import java.lang.Number;

rule "find-conflicting-songs"
	dialect "mvel"
	when
		$firstSong : SongEvent( $name : name)
		$secondSong : SongEvent( $nameTwo : name == $name , this meets [5m] $firstSong , this != $firstSong )
	then
		System.out.println($nameTwo + " meets " + $name)
		retract( $firstSong );
end
